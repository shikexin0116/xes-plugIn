{
	"name": "xue-plugin",
	"displayName": "Xes Snippets",
	"version": "0.0.1",
	"publisher": "xes-skx",
	"categories": [
		"Snippets"
	],
	"description": "xue Snippets for VS Code",
	"icon": "images/logo.png",
	"keywords": [
		"xue",
		"mobile",
		"vue",
		"snippets"
	],
	"galleryBanner": {
		"color": "#549BFF",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:vuejs"
	],
	"engines": {
		"vscode": "^1.0.0"
	},
	"main": "./out/extension",
	"devDependencies": {
		"vscode": "0.10.x"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/shikexin0116/xes-plugIn/issues",
		"email": "shikexin0116@163.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shikexin0116/xes-plugIn"
	},
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/element-ui.json"
			},
			{
				"language": "html",
				"path": "./snippets/element-ui.json"
			},
			{
				"language": "vue",
				"path": "./snippets/xes.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/xes.json"
			}
		]
	}
}